# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type UserCart {
    UserId: ID!
    Username: String
    Password: String
    DateOfBirth: String
    Age: Int
    Email: String
    CartNumber: Int
}


type Person {
  id: Int!
  name: String!
  pet: Pet
}

type Pet {
  id: Int!
  name: String!
}

type Query {
  getPerson: [Person!]!
  getpPet: [Pet!]!
  getUserCart:[UserCart!]!
}



